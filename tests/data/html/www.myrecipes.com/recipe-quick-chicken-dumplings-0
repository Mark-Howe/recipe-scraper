      <script>
        function _pl_show_video() {
        document.getElementsByClassName('pane-node-field-images')[0].style.display = 'none';
                document.getElementById('recipe-hero-video-wrap').style.display = 'block';
                bcJumpstart.playVideoOrAd(document.getElementById('recipe-hero-video'));
        }
