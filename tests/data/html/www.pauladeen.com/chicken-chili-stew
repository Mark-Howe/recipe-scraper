<script type="text/javascript">
    //<![CDATA[
    var reviewsize = 4;
    var moreInProgress = false;
    $j("a[data-review-more]").click(function(event) {
        event.preventDefault();
        if (moreInProgress) {
            return;
        }
        moreInProgress = true;
        var element = this;
        var data = { cur_page: $j(element).attr('data-page') || 2, product_id: 1732};
        $j.ajax({
            url: 'http://www.pauladeen.com/review/product/getMore/dir/desc/',
            data: data,
            type: 'post',
            dataType: 'json',
            success: function(response) {
                if (response.content.length > 0) {
                    var cur_page = $j(element).attr('data-page') || 2;
                    if (reviewsize <= cur_page * 10) {
                        $(element).hide();
                    }
                    $j("a[data-review-more]").each(function(){
                        $j(this).attr('data-page', response.cur_page);
                    });
                    $j('div[data-review-list-more]').each(function() {
                        $j(this).append(response.content)
                    });
                } else {
                    $(element).hide();
                }
                moreInProgress = false;
            }
        });
    });
    //]]>
</script>
