        <script type="text/javascript">
    //<![CDATA[
    var relatedsize = 3;
    $j("#products_more").click(function(event) {
        event.preventDefault();

        var element = this;
        var data = { cur_page: $j(element).attr('data-page') || 2, product_id: 1732};
        $j.ajax({
            url: 'http://www.pauladeen.com/zynovo_catalog/product_related/getMore/',
            data: data,
            type: 'post',
            dataType: 'json',
            success: function(response) {
                if (response.content.length > 0) {
                    var cur_page = $j(element).attr('data-page') || 2;
                    if (relatedsize <= cur_page*4) {
                        $(element).hide();
                    }
                    $j(element).attr('data-page', response.cur_page);
                    $j('#block_related .mini-products-list').append(response.content);
                } else {
                    $(element).hide();
                }
            }
        });
    });
    //]]>
    </script>
</div>
