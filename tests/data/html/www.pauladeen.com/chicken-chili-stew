<script></script>
<script>
	window.PDN = {};

	window.PdVideo = {
		freePlaysLimit: 0
		, checkFreePlays: function(player, isActiveSubscriber, isVideoFree)
		{
			if (isActiveSubscriber || isVideoFree) {
				return;
			} else {
				this.redirectToSubscribe();
			}
		}
		, zypeVideoCallback: function()
		{
			this.checkFreePlays(false, window.PDN.isActiveSubscriber, window.PDN.isVideoFree);
		}
		,redirectToSubscribe: function()
		{
			window.location.href = '/subscription/subscribe/';
		}
	}

</script>


