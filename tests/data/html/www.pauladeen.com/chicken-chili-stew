<script>
var referrals = {
									"http://start.pauladeen.com/evine-subscribe-page/": {name:"evine",period:15}
}
var incoming = document.referrer;

jQuery.each(referrals, function(key, value) {
	if(incoming == key) {
		var date = new Date(), futureDate = new Date();
		futureDate.setDate(futureDate.getDate() + value.period);
		var referral = value.name + '|' + date + '|' + futureDate;
		jQuery.cookie('referral', referral, { domain: 'pauladeen.com' });
		console.log(referral);
	}
});
</script>

