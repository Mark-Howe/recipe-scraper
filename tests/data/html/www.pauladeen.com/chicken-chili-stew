<div class="box-collateral box-reviews" id="customer-reviews">
    <script>
$j(document).ready(function(){

    $j('.ratings_stars').hover(
        // Handles the mouseover
        function() {
            $j(this).prevAll().andSelf().addClass('ratings_over');
            $j(this).nextAll().removeClass('ratings_vote');
        },
        // Handles the mouseout
        function() {
            $j(this).prevAll().andSelf().removeClass('ratings_over');
            set_votes($j(this).parent().id);
        }
    );

    $j('.ratings_stars').bind('click', function() {
        var star = this;
        var widget = $j(this).parent();
        var rateSelect = $j(this).attr('id');

        $j('.radio').attr('checked', false);
        set_votes(rateSelect);

    });

    $j('#cancel').bind('click', function(){

        $j('.radio').attr('checked', false);
        $j('.ratings_stars').removeClass('ratings_vote');
    });
    function set_votes(widget) {
        $j('input#'+widget).prop('checked', true);
        $j('div#'+widget).prevAll().andSelf().addClass('ratings_vote');
        $j('div#'+widget).nextAll().removeClass('ratings_vote');
    }
});
</script>
