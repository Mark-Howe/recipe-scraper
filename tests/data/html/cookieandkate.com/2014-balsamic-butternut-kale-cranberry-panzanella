        "servingSize": null,
        "calories": "511",
        "sugarContent": "21.1 g",
        "sodiumContent": "562.7 mg",
        "fatContent": "35.3 g",
        "saturatedFatContent": "7.6 g",
        "transFatContent": "0 g",
        "carbohydrateContent": "43.6 g",
        "fiberContent": "5.8 g",
        "proteinContent": "11.8 g",
        "cholesterolContent": "14.5 mg",
        "@type": "nutritionInformation"
    }
}
