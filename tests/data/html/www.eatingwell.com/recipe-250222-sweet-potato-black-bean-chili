        <ul class="multiColumn listIngredients clearfix" ng-hide="ingredientsReloaded">
                    <li class="checkListListItem checkListLine ">
                        <span itemprop="ingredients">1 tablespoon plus 2 teaspoons extra-virgin olive oil</span>
                    </li>
                    <li class="checkListListItem checkListLine ">
                        <span itemprop="ingredients">1 medium-large sweet potato, peeled and diced</span>
                    </li>
                    <li class="checkListListItem checkListLine ">
                        <span itemprop="ingredients">1 large onion, diced</span>
                    </li>
                    <li class="checkListListItem checkListLine ">
                        <span itemprop="ingredients">4 cloves garlic, minced</span>
                    </li>
                    <li class="checkListListItem checkListLine ">
                        <span itemprop="ingredients">2 tablespoons chili powder</span>
                    </li>
                    <li class="checkListListItem checkListLine ">
                        <span itemprop="ingredients">4 teaspoons ground cumin</span>
                    </li>
                    <li class="checkListListItem checkListLine ">
                        <span itemprop="ingredients">½ teaspoon ground chipotle chile (see Note)</span>
                    </li>

        </ul>
        <ul class="multiColumn listIngredients clearfix" ng-hide="ingredientsReloaded">
                    <li class="checkListListItem checkListLine ">
                        <span itemprop="ingredients">¼ teaspoon salt</span>
                    </li>
                    <li class="checkListListItem checkListLine ">
                        <span itemprop="ingredients">2½ cups water</span>
                    </li>
                    <li class="checkListListItem checkListLine ">
                        <span itemprop="ingredients">2 15-ounce cans black beans, rinsed</span>
                    </li>
                    <li class="checkListListItem checkListLine ">
                        <span itemprop="ingredients">1 14-ounce can diced tomatoes</span>
                    </li>
                    <li class="checkListListItem checkListLine ">
                        <span itemprop="ingredients">4 teaspoons lime juice</span>
                    </li>
                    <li class="checkListListItem checkListLine ">
                        <span itemprop="ingredients">½ cup chopped fresh cilantro</span>
                    </li>

        </ul>

    
