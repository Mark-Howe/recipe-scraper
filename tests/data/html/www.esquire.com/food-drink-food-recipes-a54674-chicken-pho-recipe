data-share-url="http://www.esquire.com/food-drink/food/recipes/a54674/chicken-pho-recipe/" data-share-media="http://esq.h-cdn.co/assets/17/16/1600x988/gallery-1492789553-hi-res-shutterstock-533096989.jpg" data-share-description="How%20to%20Make%20Vietnamese%20Chicken%20Pho"
