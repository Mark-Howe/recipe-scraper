    data-recipe-drawer='{"analyticsEvent": "NutritionExpand" }' data-bind="css: { expanded: isExpanded() }">

    <a class="showHideToggleButton" data-bind="click: toggleState"><span></span></a>

    <span id="gmi_rp_nutrition_information_label">
        <h2 id="gmi_rp_nutrition_header">Nutrition Information</h2>
