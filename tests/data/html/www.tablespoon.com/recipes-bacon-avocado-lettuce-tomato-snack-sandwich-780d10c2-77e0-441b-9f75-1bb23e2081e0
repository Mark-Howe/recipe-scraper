    <input class="monthPart"
        type="number"
        pattern="\d*"
        min="1"
        max="12"
        maxlength="2"
        data-bind="value: $root.MonthPart,
    numericAutoTab: 2,
    valueUpdate: ['onLoad', 'input', 'afterKeyUp', 'blur'],    
    attr : { 'id': ControlIds.BirthdateMonth, 'name': 'birthdateMonth', 'placeholder': $root.monthWatermarkText }" />
</script>
