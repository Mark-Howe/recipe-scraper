            <div class="savings-error-content" data-bind="html: configuration.Settings.ContentWhenPrintingFails"></div>
            <button type="button" class="savings-error-cancelButton" data-bind="text: configuration.Settings.ButtonTextWhenPrintingFails, click: switchToListView"></button>
        </div>

