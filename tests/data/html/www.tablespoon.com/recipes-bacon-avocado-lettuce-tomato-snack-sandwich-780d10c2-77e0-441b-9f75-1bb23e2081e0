                <fieldset data-bind="foreach: LoginStepFields">
                    <div data-bind="template: ViewTemplateName"></div>
                </fieldset>
                <fieldset data-bind="foreach: LoginStepNewsletterFields">
                    <div data-bind="template: ViewTemplateName"></div>
                </fieldset>
                <div class="forgotPasswordContainer">
                    <a href="#" class="forgotPasswordLink" data-bind="attr: { id: $root.loginStepId + 'forgotPassword' },text: ForgotPasswordText, click: SwitchToForgotPasswordView"></a>
                </div>

                <div class="regButtonContainer">
                    <!-- ko foreach: LoginStepButtons -->
                    <button type="button" data-bind="html: DisplayText, click: $parent.ButtonClick.bind($data, $parent), attr: { 'id':  $root.loginStepId + Id, 'data-action': Action, 'class': Action  }"></button>
                    <!-- /ko -->
                </div>
                
                <div class="loginFederatedCTA" data-bind="template: { 'name': 'gigyaFederatedCTATemplate', 'if': GigyaLoginIsEnabled }"></div>
            </div>
        </div>

        <div class="registrationView" data-bind="attr: { 'data-step-id': 'SocialLinkView' }, visible: CurrentView() === window.GeneralMills.Registration.Settings.socialLinkViewName, enterKey: FederatedLink">
            <div class="registrationSteps">
                <div class="registrationStep">
                    <div class="registrationStepInfo" data-bind="attr: { 'data-step-id': Id, 'data-step-number': 0, 'class': FederatedCurrentUserCssClass }">
                        <h2 data-bind="text: FederatedCurrentUserStepName"></h2>
                        <span data-bind="html: FederatedCurrentUserHeaderText"></span>

