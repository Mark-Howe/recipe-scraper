                            <div data-bind="template: ViewTemplateName"></div>
                        </fieldset>
                        <div class="regButtonContainer">
                            <!-- ko foreach: FederatedCurrentUserDisplayButtons -->
                            <button type="button" data-bind="html: DisplayText, click: $root.ButtonClick.bind($data, $parent), attr: { 'id': Id, 'data-action': Action, 'class': Action  }"></button>
                            <!-- /ko -->
                        </div>
                        <span data-bind="html: FederatedCurrentUserFooterText"></span>
                    </div>
                </div>
                <span class="alert alert-error" data-bind="text: $root.error(), visible: $root.error()"></span>
            </div>
            <div data-bind="visible: $root.CurrentStep() == 0">
                
            </div>
        </div>

        <div class="registrationView" data-bind="template: {name: '_registration.registrationStep.tmpl', data: {'RegistrationSteps': RegistrationSteps, 'ViewName': 'RegistrationView'}}, attr: { 'data-step-id': 'RegistrationView' }, visible: CurrentView() === window.GeneralMills.Registration.Settings.registrationViewName, enterKey: RegistrationEnterKeyPress"></div>
        <div class="registrationView" data-bind="template: {name: '_registration.registrationStep.tmpl', data: {'RegistrationSteps': SocialRegistrationSteps, 'ViewName': 'RegistrationView'}}, attr: { 'data-step-id': 'SocialRegistrationView' }, visible: CurrentView() === window.GeneralMills.Registration.Settings.socialRegViewName, enterKey: RegistrationEnterKeyPress"></div>

        <span class="alert alert-error" data-bind="visible: CurrentView() === window.GeneralMills.Registration.Settings.socialRegViewName && GigyaLoginIsEnabled && SocialRegistrationSteps.length == 0">There are no social login steps defined in Sitecore for this flow!</span>

        <div class="legalAgeView" data-step-id="legalAgeView" data-bind="visible: CurrentView() === window.GeneralMills.Registration.Settings.validAgeErrorViewName">
            <h2 data-bind="text: GlobalConfiguration.LegalAgeHeaderText"></h2>
            <span data-bind="html: GlobalConfiguration.LegalAgeErrorText"></span>
