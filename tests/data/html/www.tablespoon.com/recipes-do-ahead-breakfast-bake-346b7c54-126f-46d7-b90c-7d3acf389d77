    <select class="yearPart"
        required
        data-bind="value: $root.YearPart,
    options: $root.YearSelectOptions,
    optionsCaption: $root.yearWatermarkText,
    attr: {
        'id': $root.ControlIds.BirthdateYear,
        'name': 'birthdateYear'
    }">
    </select>
</script>
<script id="_newsletterCta.dateofbirthdropdown.tmpl" type="text/html">    
    <!-- Possible date orders are YMD, DMY, MDY (see http://en.wikipedia.org/wiki/Date_format_by_country) -->

    <!-- ko if: $root.DatePattern.HasYearBeforeMonthAndDay() -->
    <div class="dropdown-container" data-bind="template: '_newsletterCta.dateofbirthdropdown.year.tmpl'"></div>
    <!-- /ko -->

    <!-- ko if: $root.DatePattern.HasMonthBeforeDay() -->
    <div class="dropdown-container" data-bind="template: '_newsletterCta.dateofbirthdropdown.month.tmpl'"></div>
    <div class="dropdown-container" data-bind="template: '_newsletterCta.dateofbirthdropdown.day.tmpl'"></div>
    <!-- /ko -->

    <!-- ko if: !$root.DatePattern.HasMonthBeforeDay() -->
    <div class="dropdown-container" data-bind="template: '_newsletterCta.dateofbirthdropdown.day.tmpl'"></div>
    <div class="dropdown-container" data-bind="template: '_newsletterCta.dateofbirthdropdown.month.tmpl'"></div>
    <!-- /ko -->

    <!-- ko if: !$root.DatePattern.HasYearBeforeMonthAndDay() -->
    <div class="dropdown-container" data-bind="template: '_newsletterCta.dateofbirthdropdown.year.tmpl'"></div>
    <!-- /ko -->    
</script>

