    <div class="textbox-container" data-bind="template: '_newsletterCta.dateofbirthtextbox.month.tmpl'"></div>
    <!-- /ko -->

    <!-- ko if: !$root.DatePattern.HasYearBeforeMonthAndDay() -->
    <div class="textbox-container" data-bind="template: '_newsletterCta.dateofbirthtextbox.year.tmpl'"></div>
    <!-- /ko -->    
</script>

