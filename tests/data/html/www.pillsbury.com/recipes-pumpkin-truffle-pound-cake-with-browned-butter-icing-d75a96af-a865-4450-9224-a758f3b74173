</script>
<script id="_newsletterCta.dateofbirthtextbox.year.tmpl" type="text/html">
    <input class="yearPart"
        type="number"
        pattern="\d*"
        min="1900"
        maxlength="4"
        data-bind="value: $root.YearPart,
    numericAutoTab: 4,
    valueUpdate: ['onLoad', 'input', 'afterKeyUp', 'blur'],        
    attr : {
        'required': true,
        'id': ControlIds.BirthdateYear,
        'name': 'birthdateYear',
        'max': $root.CurrentYear,
        'placeholder': $root.yearWatermarkText
    }" />
</script>
