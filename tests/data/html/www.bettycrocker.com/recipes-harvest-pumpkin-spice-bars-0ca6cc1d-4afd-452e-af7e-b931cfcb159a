<script id="_newsletterCta.dateofbirthtextbox.month.tmpl" type="text/html">
    <input class="monthPart"
        type="number"
        pattern="\d*"
        min="1"
        max="12"
        maxlength="2"
        data-bind="value: $root.MonthPart,
    numericAutoTab: 2,
    valueUpdate: ['onLoad', 'input', 'afterKeyUp', 'blur'],    
    attr : { 'id': ControlIds.BirthdateMonth, 'name': 'birthdateMonth', 'placeholder': $root.monthWatermarkText }" />
</script>
<script id="_newsletterCta.dateofbirthtextbox.day.tmpl" type="text/html">
    <input class="dayPart"
        type="number"
        pattern="\d*"
        min="1"
        max="31"
        maxlength="2"
        data-bind="value: $root.DayPart,
    numericAutoTab: 2,
    valueUpdate: ['onLoad', 'input', 'afterKeyUp', 'blur'],    
    attr : { 'id': ControlIds.BirthdateDay, 'name': 'birthdateDay', 'placeholder': $root.dayWatermarkText }" />
</script>
<script id="_newsletterCta.dateofbirthtextbox.year.tmpl" type="text/html">
    <input class="yearPart"
        type="number"
        pattern="\d*"
        min="1900"
        maxlength="4"
        data-bind="value: $root.YearPart,
    numericAutoTab: 4,
    valueUpdate: ['onLoad', 'input', 'afterKeyUp', 'blur'],        
    attr : {
        'required': true,
        'id': ControlIds.BirthdateYear,
        'name': 'birthdateYear',
        'max': $root.CurrentYear,
        'placeholder': $root.yearWatermarkText
    }" />
</script>
