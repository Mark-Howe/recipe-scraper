    <div class="textbox-container" data-bind="template: '_newsletterCta.dateofbirthtextbox.day.tmpl'"></div>
    <!-- /ko -->

    <!-- ko if: !$root.DatePattern.HasMonthBeforeDay() -->
    <div class="textbox-container" data-bind="template: '_newsletterCta.dateofbirthtextbox.day.tmpl'"></div>
    <div class="textbox-container" data-bind="template: '_newsletterCta.dateofbirthtextbox.month.tmpl'"></div>
    <!-- /ko -->

