{
  "name": "ssnepenthe/recipe-parser",
  "description": "A recipe parsing library.",
  "homepage": "https://github.com/ssnepenthe/recipe-parser",
  "license": "GPL-2.0",
  "require": {
    "php": ">=5.4",
    "doctrine/cache": "^1.6",
    "jonnyw/php-phantomjs": "^4.4",
    "symfony/console": "~2.7||~3.0",
    "symfony/css-selector": "^3.0"
  },
  "require-dev": {
    "psy/psysh": "^0.7",
    "squizlabs/php_codesniffer": "^2.6",
    "phpunit/phpunit": "^5.3"
  },
  "autoload": {
    "psr-4": { "SSNepenthe\\RecipeParser\\": "src/" }
  },
  "autoload-dev": {
    "classmap": [
      "tests/CacheableHTTPTestCase.php",
      "tests/Parsers/ParserTestCase.php"
    ]
  },
  "scripts": {
    "post-install-cmd": "PhantomInstaller\\Installer::installPhantomJS",
    "post-update-cmd": "PhantomInstaller\\Installer::installPhantomJS"
  },
  "bin": ["bin/recipe"]
}